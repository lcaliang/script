+static int pn518_spi_read(char* cmd,int per_word,int count,struct pn518_data *pdata)
+{
+        void *spi_data;
+        int ret,i,ibytes=1,num_trans=0;
+        struct spi_device *spi = pdata->spidev;
+	struct spi_transfer * xfer=NULL;
+	u8 * rddata8;
+	u16 * rddata16;
+	
+       ACDBG("%s:read:%x,spi=%x\n",__func__,(unsigned int)pdata,(unsigned int)spi);
+       i=30;
+       while(i>0 && gpio_get_value(pdata->busy)== 0)  //busy active is low ,it can be configured LOW or HIGH
+       {
+	  ACDBG("--pn518 is busy");
+	  usleep(5);
+	  i--;
+       }
+
+        if(per_word > 8)
+	{
+	   ibytes = 2;
+	}
+        spi_data = kzalloc(sizeof(struct spi_transfer)*count+count*ibytes, GFP_KERNEL);
+        if (!spi_data)
+                return -ENOMEM;
+       spi_message_init(&pdata->spi_msg);
+       xfer =(struct spi_transfer*) (spi_data + count*ibytes);
+
+       ACDBG("%s:1\n",__func__);
+       for(i=0;i<count;i++)
+       {
+          xfer->bits_per_word = 8;
+          xfer->len = 1;
+          xfer->speed_hz = 4800000;
+          xfer->delay_usecs = 5;
+          xfer->rx_buf = (void*)((u8*)spi_data+i*ibytes);
+          xfer->tx_buf = NULL;
+          xfer->cs_change = 0;
+          spi_message_add_tail(xfer, &pdata->spi_msg);
+	  xfer++;
+	  num_trans++;
+       }
+        //gpio_set_value_cansleep(88,0);
+	//udelay(1);
+	ret = spi_sync(spi, &pdata->spi_msg);
+	//udelay(1);
+        //gpio_set_value_cansleep(88,1);
+       gpio_tlmm_config(GPIO_CFG(88, 0, GPIO_CFG_OUTPUT, GPIO_CFG_PULL_UP, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+       gpio_set_value(88,1);
+       usleep(5);
+       gpio_tlmm_config(GPIO_CFG(88, 1, GPIO_CFG_OUTPUT, GPIO_CFG_PULL_UP, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+
+       if (ret < 0)
+                ACDBG("%s:read: spi_sync() failed,%d\n",__func__,ret);
+
+	for(i=0;i<num_trans;i++)  
+	{
+	  if(ibytes ==1)
+	  {
+            rddata8 = (u8*)(spi_data+i);
+          ACDBG("spi_rd:%x\n",*rddata8);
+	  }
+	  else
+	  {
+            rddata16 = (u16*)(spi_data+i*ibytes);
+          ACDBG("spi_rd:%x\n",*rddata16);
+	  }
+	}
+        memcpy((void*)cmd,spi_data,ibytes*num_trans); 
+	if(ret >= 0)
+	{
+	  ret = num_trans;
+	}
+	kfree(spi_data);
+
+	return ret;
+}
