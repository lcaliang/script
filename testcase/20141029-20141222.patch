+		.pm	= &pn518_pm_ops,
+		.of_match_table = act_spi_pn518_table,
+    },
+};
+
+static int __init pn518_init(void)
+{
+   return spi_register_driver(&pn518_driver);
+}
+
+static void __exit pn518_exit(void)
+{
+   return spi_unregister_driver(&pn518_driver);
+}
+
+late_initcall(pn518_init);
