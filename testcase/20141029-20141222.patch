+static int pn518_spi_write(char* cmd,int per_word,int count,struct pn518_data *pdata)
+{
+        void *spi_data;
+        int ret=0,i,ibytes=1,num_trans=0;
+        struct spi_device *spi = pdata->spidev;
+	struct spi_transfer * xfer=NULL;
+        unsigned int reg = 0;
+	
+       ACDBG("%s:write:%x,spi=%x\n",__func__,(unsigned int)pdata,(unsigned int)spi);
+       i=30;
+       while(i>0 && gpio_get_value(pdata->busy) == 0) //busy active is low ,it can be configured LOW or HIGH
+       {
+	  ACDBG("--pn518 is busy:%d",i);
+	  usleep(5);
+	  i--;
+       }
+
+        if(per_word > 8)
+	{
+	   ibytes = 2;
+	}
+        spi_data = kzalloc(sizeof(struct spi_transfer)*count+count*ibytes, GFP_KERNEL);
+        if (!spi_data)
+                return -ENOMEM;
+
+        spi_message_init(&pdata->spi_msg);
+
+       memcpy(spi_data, (void*)cmd, ibytes*count); 
+       xfer =(struct spi_transfer*) (spi_data + count*ibytes);
+
+       for(i=0;i<count;i++)
+       {
+          xfer->bits_per_word = per_word;
+          xfer->len = 1;
+          xfer->speed_hz = 4800000;
+          xfer->delay_usecs = 5;
+          xfer->rx_buf = NULL;
+          xfer->tx_buf =(void*)((u8*)spi_data+i*ibytes);
+          //if(i==count-1)
+          //  xfer->cs_change = 1;
+          spi_message_add_tail(xfer, &pdata->spi_msg);
+        
+	  xfer++;
+	  num_trans++;
+	  if(ibytes ==1)
+          ACDBG("spi_wr:%x\n",*((u8*)spi_data+i));
+	  else
+          ACDBG("spi_wr:%x\n",*((u16*)spi_data+i));
+       }
+       //gpio_set_value(88,0);
+       //udelay(80);
+       //gpio_set_value(88,1);
+       reg = *(unsigned int*)(pdata->devbase + 304) ;
+       reg = reg | 0x0800;
+       ACDBG("%s:reg:%x\n",__func__,reg);
+       //*(unsigned int*)(pdata->devbase + 304) = reg;
+       ret = spi_sync(spi, &pdata->spi_msg);
+       gpio_tlmm_config(GPIO_CFG(88, 0, GPIO_CFG_OUTPUT, GPIO_CFG_PULL_UP, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+       gpio_set_value(88,1);
+       usleep(5);
+       gpio_tlmm_config(GPIO_CFG(88, 1, GPIO_CFG_OUTPUT, GPIO_CFG_PULL_UP, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+         
+       //reg = reg | 0x0F0;
+       //ACDBG("%s:reg:%x\n",__func__,reg);
+       //*(unsigned int*)(pdata->devbase + 304) = reg;
+       
+
+	kfree(spi_data);
+	if(ret >= 0)
+	{
+	  ret = num_trans;
+	}
+
+	return ret;
+}
