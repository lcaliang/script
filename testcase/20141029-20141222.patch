+{
+
+    struct pn518_data *pdata = dev_get_drvdata(&spi->dev);
+    kfree(pdata);
+
+    spi_set_drvdata(spi, NULL);
+
+    return 0;
+}
+
+#if  defined(CONFIG_PM_SLEEP)
+static int pn518_pm_suspend(struct device *dev)
+{
+ printk("%s,suspend\r\n",__func__);
+
+  pn518_is_suspended=1;
+    return 0;
+}
+
+static int pn518_pm_resume(struct device *dev)
+{
+ printk("%s,resume\r\n",__func__);
+  pn518_is_suspended=0;
+    return 0;
+}
+
+static SIMPLE_DEV_PM_OPS(pn518_pm_ops, pn518_pm_suspend, pn518_pm_resume);
+#endif
+
+
+static struct spi_driver pn518_driver = {
+    .probe		= pn518_probe,
+    .remove		= __devexit_p(pn518_remove),
+    .driver		= {
+		.name	= "pn518",
