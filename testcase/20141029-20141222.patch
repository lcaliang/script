+static int __devinit pn518_probe(struct spi_device *spi)
+{
+    int err = -1;
+    struct pn518_data *pdata;
+
+    enum of_gpio_flags flags;
+    unsigned int irq_pin=79;
+
+    int cs,irq;
+    int cpha,cpol,cs_high;
+    u32 max_speed;
+
+
+    pdata = kzalloc(sizeof(struct pn518_data), GFP_KERNEL);
+    if(pdata)
+    {
+      memset(pdata,0, sizeof(struct pn518_data));
+      pnfcdata = pdata;
+    }
+    if (!pdata) {
+        err = -ENOMEM;
+        goto err_free_mem;
+    }
+    err = misc_register(&spiplus_device);
+    if (err) {
+	printk(KERN_ERR "spiplus device register failed\n");
+        goto err_free_mem;
+    }
+    pdata->spidev = spi;
+
+   if(spi->dev.of_node){
+     irq_pin = of_get_named_gpio_flags(spi->dev.of_node, "pn518,irq-gpio", 0, &flags);
+     pdata->busy = of_get_named_gpio_flags(spi->dev.of_node, "pn518,busy-gpio", 0, &flags);
+     pdata->pwr3 = of_get_named_gpio_flags(spi->dev.of_node, "pn518,3v3pwr-gpio", 0, &flags);
+     pdata->pwr5 = of_get_named_gpio_flags(spi->dev.of_node, "pn518,5vpwr-gpio", 0, &flags);
+     pdata->reset = of_get_named_gpio_flags(spi->dev.of_node, "pn518,reset-gpio", 0, &flags);
+   }
+   pdata->irq = gpio_to_irq(irq_pin);
+   irq = spi->irq;
+   cs = spi->chip_select;
+   cpha = ( spi->mode & SPI_CPHA ) ? 1:0;
+   cpol = ( spi->mode & SPI_CPOL ) ? 1:0;
+   cs_high = ( spi->mode & SPI_CS_HIGH ) ? 1:0;
+   max_speed = spi->max_speed_hz;
+   dev_err(&spi->dev, "gpio [%d] irq [%d] gpio_irq [%d] cs [%x] CPHA [%x] CPOL[%x] CS_HIGH [%x]\n",
+      irq_pin, irq, pdata->irq, cs, cpha, cpol, cs_high);
+   dev_err(&spi->dev, "Max_speed [%d]\n", max_speed );
+   err = spi_setup(spi);
+   if (err) {
+                dev_dbg(&spi->dev, "spi master doesn't support 16 bits/word\n");
+                return err;
+   }
+
+    if(gpio_is_valid(irq_pin))
+    {
+        err = gpio_request(irq_pin, "pn518_irq");
+        if(err)
+        {
+            printk(KERN_ERR " pn518 request irq gpio FAIL \n");
+            goto err_free_mem;
+        }
+        gpio_tlmm_config(GPIO_CFG(irq_pin, 0, GPIO_CFG_INPUT, GPIO_CFG_PULL_UP, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+    }
+    else
+    {
+        printk(KERN_ERR " pn518 irq gpio is invalid \n");
+        err = -EIO;
+        goto err_free_mem;
+    }
+
+    ACDBG("%s:5vpwr=%d\n",__func__,pdata->pwr5);
+    if(gpio_is_valid(pdata->pwr5))
+    {
+        err = gpio_request(pdata->pwr5, "pn518_5vpwr");
+        if(err)
+        {
+            printk(KERN_ERR " pn518 request 5vpwr gpio FAIL \n");
+            goto err_free_mem;
+        }
+        gpio_tlmm_config(GPIO_CFG(pdata->pwr5, 0, GPIO_CFG_OUTPUT, GPIO_CFG_NO_PULL, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+	gpio_set_value(pdata->pwr5,1);
+    }
+    else
+    {
+       printk(KERN_ERR " pn518 pwr5 gpio is invalid \n");
+       err = -EIO;
+       goto err_free_mem;
+    }
+    ACDBG("%s:3vpwr=%d\n",__func__,pdata->pwr3);
+    if (gpio_is_valid(pdata->pwr3)) {
+	err = gpio_request(pdata->pwr3, "pn518_3v3pwr");
+	if (err) {
+		dev_err(&spi->dev, "pn518 3v3 pwr gpio request failed");
+		goto err_free_mem;
+	}
+
+	err = gpio_direction_output(pdata->pwr3, 0);
+	if (err) {
+		dev_err(&spi->dev,
+			"set_direction for 3v3pwr gpio failed\n");
+		goto err_free_mem;
+	}
+	gpio_set_value_cansleep(pdata->pwr3, 1);
+    }
+    ACDBG("%s:busy=%d\n",__func__,pdata->busy);
+    if(gpio_is_valid(pdata->busy))
+    {
+        err = gpio_request(pdata->busy, "pn518_busy");
+        if(err)
+        {
+            printk(KERN_ERR " pn518 request busy gpio FAIL \n");
+            goto err_free_mem;
+        }
+        gpio_tlmm_config(GPIO_CFG(pdata->busy, 0, GPIO_CFG_INPUT, GPIO_CFG_NO_PULL, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+    }
+    else
+    {
+       printk(KERN_ERR " pn518 busy gpio is invalid \n");
+       err = -EIO;
+       goto err_free_mem;
+    }
+    ACDBG("%s:reset=%d\n",__func__,pdata->reset);
+    if (gpio_is_valid(pdata->reset)) {
+	err = gpio_request(pdata->reset, "pn518_reset");
+	if (err) {
+		dev_err(&spi->dev, "pn518 reset gpio request failed");
+		goto err_free_mem;
+	}
+
+	err = gpio_direction_output(pdata->reset, 0);
+	if (err) {
+		dev_err(&spi->dev,
+			"set_direction for reset gpio failed\n");
+		goto err_free_mem;
+	}
+	gpio_set_value_cansleep(pdata->reset, 1);
+        usleep(20);
+	gpio_set_value(pdata->reset,0);
+        usleep(20);
+	gpio_set_value(pdata->reset,1);
+    }
+  
+    //gpio_tlmm_config(GPIO_CFG(88, 0, GPIO_CFG_OUTPUT, GPIO_CFG_PULL_UP, GPIO_CFG_2MA), GPIO_CFG_ENABLE);
+    //gpio_set_value(88,1);
+   
+
+
+    err = request_threaded_irq(pdata->irq, pn518_hard_irq, pn518_irq_handler,
+				     /*IRQF_TRIGGER_RISING |*/ IRQF_TRIGGER_FALLING, "pn518", pdata);
+
+
+    if (err < 0) 
+    {
+        printk(KERN_ERR " pn518 request irq fail \n");
+        goto err_free_mem;
+    }
+
+
+    printk(KERN_ERR " %s:dev_name: %s \n",__func__,dev_name(&spi->dev));
+
+    dev_set_drvdata(&spi->dev, pdata);
+
+	pdata->devbase =(void*) ioremap(0xF9926000,0x1000);
+	if (!pdata->devbase) {
+		err = -ENOMEM;
+		goto err_free_mem;
+	}
+    ACDBG("devbase = %x ,  \n",(unsigned int)pdata->devbase);
+
+
+    printk(KERN_ERR " pn518_probe OK \n");
+    return 0;
+
+
+err_free_mem:
+    if(pdata)
+    {
+    kfree(pdata);
+    }
+
+    printk(KERN_ERR " pn518_probe FAIL , err = %x\n", err);
+
+    return err;
+}
